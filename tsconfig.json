{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["ES2019", "ES2020"],
    "target": "ES2020",
    "types": ["node"],
    "resolveJsonModule": true,
    "declaration": false,
    "inlineSourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}